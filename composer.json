{
	"name" : "raincity/php-library",
	"description" : "Library to support implementations in PHP",
	"type" : "library",
	"version" : "2.1.58",
	"license" : "BSD-3-Clause",
	"minimum-stability" : "stable",
	"require" : {
		"php" : ">=8.2",
		"composer/installers" : "~2.3",
		"psr/log" : "~3.0",
		"monolog/monolog" : "~3.8",
		"psr/simple-cache" : "~3.0",
		"symfony/cache" : "~7.1",
		"guzzlehttp/guzzle" : "~7.9",
		"json-mapper/json-mapper" : "~2.22",
		"doctrine/annotations" : "~2.0"
	},
	"autoload" : {
		"classmap" : [
			"src/"
		],
		"psr-4" : {
			"" : [
				"src",
				"test"
			]
		}
	},
	"require-dev" : {
		"phpunit/phpunit" : "~11.4",
		"brain/monkey" : "~2.6",
		"consolidation/robo" : "~5.1",
		"phpstan/phpstan" : "~2.0"
	},
	"config" : {
		"allow-plugins" : {
			"composer/installers" : true
		}
	}
}