{
	"name" : "raincity/php-library",
	"description" : "Library to support implementations in PHP",
	"type" : "library",
	"version" : "2.1.31",
	"license" : "BSD-3-Clause",
	"minimum-stability" : "stable",
	"require" : {
		"php" : ">=8.2",
		"composer/installers" : "~2.2",
		"psr/log" : "~3.0",
		"monolog/monolog" : "~3.5",
		"psr/simple-cache" : "~3.0",
		"symfony/cache" : "~6.3",
		"guzzlehttp/guzzle" : "~7.8",
		"json-mapper/json-mapper" : "~2.22",
		"doctrine/annotations" : "~2.0"
	},
	"autoload" : {
		"classmap" : [
			"src/"
		],
		"psr-4" : {
			"" : [
				"src",
				"test"
			]
		}
	},
	"require-dev" : {
		"phpunit/phpunit" : "~9.6",
		"brain/monkey" : "~2.6"
	},
	"config" : {
		"allow-plugins" : {
			"composer/installers" : true
		}
	}
}